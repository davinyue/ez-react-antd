    "antd-img-crop": "^4.27.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.11.0",
    "react-select": "^5.10.2",
    "react-window": "^2.2.3",
    "redux": "^5.0.0"
